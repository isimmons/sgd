{
    "name": "isimmons/sgd",
    "description": "Easily deploy your Sculpin generated static site to github",
    "keywords": ["sculpin", "git", "github", "deploy"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Simmons",
            "email": "isimmons33@gmail.com"
        }
    ],
    "autoload": {
        "classmap": ["src/lib"]
    },
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.1.*",
        "kherge/amend": "~3.0",
        "nategood/httpful": "~0.2",
        "symfony/console": "2.4.*",
        "symfony/process": "2.4.*"        
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "0.9.*"
    },
    "config": {
        "bin-dir": "bin",
        "vendor-dir": "src/vendor"
    },
    "minimum-stability": "stable"
}